<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- Username to connect to the Ring API -->
    <add key="RingUsername" value="" />

    <!-- Password to connect to the Ring API -->
    <add key="RingPassword" value="" />

    <!-- Refresh Token to use to authenticate to the Ring API. Will automatically be populated after logging in interactively once. -->
    <add key="RefreshToken" value="" />

    <!-- Identifier as to which recording was downloaded last. Do not modify manually. -->
    <add key="LastdownloadedRecordingId" value="" />
  </appSettings>
</configuration>